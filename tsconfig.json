{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "react-jsx",
    "module": "esnext",
    "strict": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    ".expo/types/**/*.ts",
    ".storybook/**/*",
    "**/*.ts",
    "**/*.tsx",
    "expo-env.d.ts"
  ]
}
