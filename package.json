  "scripts": {
    "bench": "NODE_OPTIONS=--disable-warning=DEP0040 yarn run reassure --testMatch=\"<rootDir>/**/*.bench.[jt]s?(x)\"",
    "bench:ci": "./scripts/bench/ci.sh",
    "build:android:test": "yarn build:android:test:debug",
    "build:android:test:base": "cd android && SENTRY_ALLOW_FAILURE=true SENTRY_DISABLE_AUTO_UPLOAD=true ./gradlew assembleAndroidTest",
    "build:android:test:debug": "yarn build:android:test:base assembleEndToEndTestDebug -DtestBuildType=debug",
    "build:android:test:release": "yarn build:android:test:base assembleEndToEndTestRelease -DtestBuildType=release",
    "build:clean": "expo prebuild --clean",
    "build:database": "drizzle-kit generate",
    "build:intl:extract-template": "lingui extract-template",
    "build:intl:compile": "lingui compile --typescript",
    "build:ios:test": "yarn build:ios:test:debug",
    "build:ios:test:base": "SENTRY_ALLOW_FAILURE=true SENTRY_DISABLE_AUTO_UPLOAD=true xcodebuild -workspace ios/Hibi.xcworkspace -scheme Hibi -sdk iphonesimulator -derivedDataPath ios/build",
    "build:ios:test:debug": "yarn build:ios:test:base -configuration Test-Debug",
    "build:ios:test:release": "yarn build:ios:test:base -configuration Test-Release",
    "build:storybook:native": "sb-rn-get-stories --config-path .storybook/native",
    "build:web": "expo start --web",
    "lint": "expo lint && prettier . --check && yarn lint:types && yarn lint:yaml && yarn lint:shell",
    "lint:fix": "eslint --fix && prettier . --write --ignore-unknown",
    "lint:shell": "shellcheck $(git ls-files '*.sh')",
    "lint:types": "tsc --project tsconfig.json --noEmit",
    "lint:yaml": "yamllint .",
    "postinstall": "husky && patch-package",
    "run:android": "expo run:android --variant developmentDebug",
    "run:ios": "SENTRY_ALLOW_FAILURE=true expo run:ios --configuration Development-Debug",
    "start": "expo start --port 8082 --scheme co.hibi.app.dev",
    "start:test": "NODE_ENV=test expo start --port 8081 --scheme co.hibi.app.test",
    "start:production": "expo start --minify --no-dev",
    "start:production:analyze-bundle": "EXPO_ATLAS=true yarn start:production",
    "start:storybook": "storybook dev --config-dir .storybook/web",
    "test": "NODE_OPTIONS=--disable-warning=DEP0040 jest",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watchAll",
    "test:end-to-end": "./scripts/test/end-to-end.sh",
    "android": "yarn run:android",
    "ios": "yarn run:ios"
  },
